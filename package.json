{
  "name": "node-libssh2",
  "version": "1.0.0",
  "description": "High-performance SSH client for Node.js with node-ssh compatible API using libssh2 native FFI bindings",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "postinstall": "node scripts/install-native.js",
    "test": "node examples/test-simple-command.js",
    "example": "node examples/test-simple-command.js",
    "example:async": "node examples/test-simple-command.js",
    "example:core": "node examples/low-level-usage.ts",
    "example:wrapper": "node examples/wrapper-usage.ts"
  },
  "keywords": [
    "ssh",
    "ssh2",
    "libssh2",
    "node-ssh",
    "terminal",
    "remote",
    "shell",
    "ffi",
    "native",
    "high-performance",
    "promises",
    "async",
    "compatible"
  ],
  "author": {
    "name": "Mohammed Imran",
    "email": "Postme.imran@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/moimran/node-libssh2.git"
  },
  "bugs": {
    "url": "https://github.com/moimran/node-libssh2/issues"
  },
  "homepage": "https://github.com/moimran/node-libssh2#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "os": [
    "win32",
    "linux",
    "darwin"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "dependencies": {
    "koffi": "^2.8.8"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "libs/**/*",
    "scripts/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],

  "publishConfig": {
    "access": "public"
  }
}
